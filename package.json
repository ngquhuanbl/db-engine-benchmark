{
	"name": "db-engine-benchmark",
	"version": "0.1.0",
	"private": true,
	"author": "huannq",
	"description": "Database engine benchmark",
	"main": "./public/electron.js",
	"homepage": "./",
	"dependencies": {
		"@chakra-ui/icons": "^1.1.7",
		"@chakra-ui/react": "^1.8.8",
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@faker-js/faker": "^7.6.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.5.2",
		"@types/node": "^16.18.6",
		"@types/react": "16.14.0",
		"@types/react-dom": "16.9.9",
		"@types/sqlite3": "^3.1.8",
		"apexcharts": "^3.36.3",
		"framer-motion": "^4.1.17",
		"react": "16.14.0",
		"react-apexcharts": "^1.4.0",
		"react-dom": "16.14.0",
		"react-scripts": "3.4.4",
		"typescript": "^4.4.2",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"electron:start": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\""
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			"electron 7.3.2"
		],
		"development": [
			"electron 7.3.2"
		]
	},
	"devDependencies": {
		"concurrently": "^7.6.0",
		"cross-env": "^7.0.3",
		"electron": "^7.3.2",
		"electron-builder": "^23.6.0",
		"electronmon": "^2.0.2",
		"wait-on": "^6.0.1"
	},
	"build": {
		"appId": "com.electron.db-engine-benchmark",
		"productName": "DB Engine Benchmark",
		"files": [
			"build/**/*",
			"node_modules/**/*"
		],
		"directories": {
			"buildResources": "public"
		},
		"mac": {
			"target": "dmg"
		},
		"win": {
			"target": "nsis"
		},
		"linux": {
			"target": "deb"
		}
	}
}
